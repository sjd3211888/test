        os << "\"";
        for (char c : key_str) {
            switch (c) {
                case '\"': os << "\\\\\\""; break;
                case '\\\\': os << "\\\\\\\\"; break;
                case '\b': os << "\\\b"; break;
                case '\f': os << "\\\f"; break;
                case '\n': os << "\\\n"; break;
                case '\r': os << "\\\r"; break;
                case '\t': os << "\\\t"; break;
